{
  "name": "tocco-client",
  "version": "0.1.3",
  "description": "Monorepo for the Tocco client",
  "author": "Tocco AG",
  "license": "AGPL-3.0+",
  "scripts": {
    "build-storybook": "build-storybook --quiet -s ./storybook/assets -c ./storybook -o ./dist/storybook",
    "changelog": "./scripts/changelog.sh",
    "clean": "rimraf node_modules && cd server/static && rimraf node_modules && lerna clean --yes",
    "compile": "babel-node build/scripts/compile",
    "compile:prod": "cross-env NODE_ENV=production npm run compile --",
    "compile:dev": "cross-env NODE_ENV=development npm run compile --",
    "lint": "eslint . --cache && stylelint 'packages/**/*.js' --cache --syntax scss",
    "lint:fix": "eslint . --cache --fix && stylelint 'packages/**/*.js' --cache --syntax scss",
    "start": "babel-node build/scripts/dev-server",
    "test": "jest",
    "codecov": "codecov --disable=gcov -f ./coverage/lcov.info",
    "setup": "yarn && cd server/static && yarn && cd .. && lerna bootstrap --ignore-scripts --force-local",
    "setup:ci": "yarn install --frozen-lockfile && lerna bootstrap --no-progress --ignore-scripts --concurrency 2 --force-local",
    "plop": "babel-node ./node_modules/plop",
    "release-package": "yarn setup && ./scripts/release-package.sh",
    "release-all-packages": "yarn setup && ./scripts/release-all-packages.sh",
    "storybook": "start-storybook -s ./storybook/assets -p 3003 -c ./storybook",
    "storybook:master": "cross-env BACKEND=https://master.tocco.ch yarn storybook",
    "cypress:localhost": "cross-env CYPRESS_baseUrl=http://localhost:3003 yarn cypress open",
    "cypress:master": "yarn cypress open",
    "cypress:master:record": "yarn cypress run --record --key 7657cc15-e969-4203-ae80-f3e0651a9667",
    "prepare": "husky install"
  },
