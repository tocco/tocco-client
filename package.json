{
  "name": "tocco-client",
  "version": "0.1.2",
  "description": "Monorepo for react.js projects using lerna",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf node_modules && cd server/static && rimraf node_modules && lerna clean",
    "compile": "babel-node build/scripts/compile",
    "compile:prod": "NODE_ENV=production npm run compile --",
    "compile:dev": "NODE_ENV=development npm run compile --",
    "compile:standalone": "NODE_ENV=standalone npm run compile --",
    "compile:nice2_11_legacy": "NODE_ENV=nice2_11_legacy npm run compile --",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "stylelint \"packages/**/*.scss\" && eslint . --cache",
    "lint:fix": "npm run lint -- --fix",
    "start": "babel-node build/scripts/dev-server",
    "test": "NODE_ENV=test babel-node ./node_modules/karma/bin/karma start build/karma.conf",
    "test:saucelabs": "NODE_ENV=test DISABLE_ISTANBUL_COVERAGE=true babel-node ./node_modules/karma/bin/karma start build/karma-saucelabs.conf",
    "test:dev": "npm run test -- --watch",
    "codecov": "cat ./coverage/lcov/lcov.info | codecov",
    "setup": "yarn && cd server/static && yarn",
    "plop": "babel-node ./node_modules/plop",
    "commitmsg": "babel-node githooks/msg-validation",
    "precommit": "npm run lint"
  },
  "author": "Tocco AG",
  "license": "AGPL-3.0+",
