{
  "name": "password-update",
  "version": "0.0.1",
  "description": "password update dialog",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "babel-node ../../bin/compile",
    "deploy": "better-npm-run deploy",
    "prepublish": "better-npm-run deploy:prod"
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile --package=merge-action",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "peerDependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "dependencies": {
    "react-redux": "^4.0.0",
    "redux": "^3.0.0",
    "redux-saga": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.9",
    "react": "latest",
    "react-dom": "latest",
    "rimraf": "^2.5.3"
  }
}
