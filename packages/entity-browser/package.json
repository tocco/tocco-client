{
  "name": "tocco-entity-browser",
  "version": "0.1.2",
  "description": "Lists entities and allows to sort and search them",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "babel-node ../../bin/compile",
    "deploy": "better-npm-run deploy",
    "prepublish": "better-npm-run deploy:prod",
    "lint": "eslint ."
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile --package=entity-browser",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "better-npm-run": "0.0.9",
    "classnames": "~2.2.4",
    "fetch-mock": "^5.5.0",
    "lodash": "^4.16.1",
    "react": "latest",
    "react-dom": "latest",
    "react-intl": "^2.1.5",
    "react-intl-redux": "^0.1.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.0.0",
    "redux-form": "^5.3.1",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.3",
    "tocco-ui": "^0.1.0",
    "tocco-util": "^0.1.0",
    "tocco-test-util": "^0.1.0"
  }
}
